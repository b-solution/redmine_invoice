<h2><%= l(:label_new_client) %></h2>
<div class="box tabular">
  <div id="all_attributes">
    <%= form_for @client, url: client_path(@client), method: 'put' do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
        <%= submit_tag l(:button_update) %>
    <% end %>
  </div>
</div>
